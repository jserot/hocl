dif top_f {

  topology {
    nodes = n8, n7, n5, n6;
    edges = e5(n7,n8), e4(n5,n7), e6(n8,n6);
    }

  actortype i {
    output o;
    }

  actortype o {
    input i;
    }

  actortype foo {
    input i;
    output o;
    }

  actor n8 {
    type: foo;
    interface e5->i, o->e6;
    }

  actor n7 {
    type: foo;
    interface e4->i, o->e5;
    }

  actor n5 {
    type: i;
    interface i->e4;
    }

  actor n6 {
    type: o;
    interface e6->o;
    }

}
