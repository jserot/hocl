dif top_s {

  topology {
    nodes = n1, n2, n3, n4;
    edges = e2(n1,n3), e1(n3,n4), e3(n4,n2);
    }

  actortype i {
    output o;
    }

  actortype o {
    input i;
    }

  actortype foo {
    input i;
    output o;
    }

  actor n1 {
    type: i;
    interface i->e2;
    }

  actor n2 {
    type: o;
    interface e3->o;
    }

  actor n3 {
    type: foo;
    interface e2->i, o->e1;
    }

  actor n4 {
    type: foo;
    interface e1->i, o->e3;
    }

}
